{
	"name": "typescript-kotlin-extensions",
	"version": "0.0.1",
	"description": "An extension library for Kotlin.",
	"main": "lib/kotlin-extensions.ts",
	"types": "lib/kotlin-extensions.d.ts",
	"scripts": {
		"test": "jest",
		"build": "tsc",
		"lint": "eslint 'src/**/*.ts'",
		"lint:fix": "eslint --fix 'src/**/*.ts'",
		"prepare": "pnpm run build",
		"prepublishOnly": "pnpm test && pnpm run lint",
		"preversion": "pnpm run lint",
		"version": "pnpm run format && git add -A src",
		"postversion": "git push && git push --tags"
	},
	"jest": {
		"transform": {
			"^.+\\.[t|j]sx?$": "ts-jest"
		}
	},
	"keywords": [
		"typescript",
		"kotlin",
		"extensions"
	],
	"author": "Taskeren <r0yalist@outlook.com>",
	"license": "MIT",
	"type": "commonjs",
	"files": [
		"lib/**/*"
	],
	"devDependencies": {
		"@types/jest": "^29.5.1",
		"@types/node": "^20.2.3",
		"jest": "^29.5.0",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"random-js": "^2.1.0"
	}
}
